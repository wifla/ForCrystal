page{
  background: #f8f8f8;
}
.memoria-container{
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: flex-start;
}
.head{
  font-size: 60rpx;
  color: #2F4F4F;
  font-weight: bold;
  margin: 20rpx 30rpx;
}
.memoria-head{
  width: 100%;
  height: 120rpx;
  display: flex;
  flex-direction: column;
  background: #fff;
}
.add-button{
  width: 180rpx;
  height: 55rpx;
  border-radius: 10rpx;
  color: #fff;
  font-size: 30rpx;
  background: rgba(255, 36, 0,0.6);
  margin-left: 30rpx;
  text-align: center;
  line-height: 55rpx;
  margin-top: 20rpx;
}
.memoria-list{
  width: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
}
.case{
  width: 750rpx;
}
.image{
  height: 360rpx;
}
.load-more{
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  font-size: 32rpx;
  color: rgba(255, 36, 0,0.6);
  font-weight: bold; 
  text-decoration: underline;
}

.cu-load{
  color: rgba(255, 36, 0,0.6);
  font-weight: bold;
  width: 100%;
}